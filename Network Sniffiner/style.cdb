	border-bottom-right-radius: 5px;
	border-top-right-radius:5px;

}
QTabWidget::tab-bar {

}

QTabBar::tab {
	
	background-color: qlineargradient(spread:pad, x1:0.527, y1:1, x2:0.510832, y2:0, stop:0 rgba(167, 177, 183, 255), stop:1 rgba(211, 218, 221, 255));
	
	border-left: 1px solid #6d838d;
	border-right: 1px solid #6d838d;
    border-top: 1px solid #6d838d;
	padding: 5px 5px 5px 5px;

	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
QTabBar::tab:selected, QTabBar::tab:hover {

}
QTabBar::tab:selected {
	background-color:#fafafa;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;

}
QTabBar::tab:!selected {

	background-color: qlineargradient(spread:pad, x1:0.527, y1:1, x2:0.510832, y2:0, stop:0 rgba(167, 177, 183, 255), stop:1 rgba(211, 218, 221, 255));
	border: 1px solid #6d838d;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;

}

