 QComboBox {
     border: 1px solid #dbe2e6;
     border-radius: 5px;
     padding: 1px 18px 1px 3px;
     min-width: 6em;
	border-bottom:2px solid #797e80;

 }

 QComboBox:editable {
     background: #5fceff;
 }

 QComboBox:!editable, QComboBox::drop-down:editable {
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0.511, y2:0, stop:0 rgba(185, 196, 200, 255), stop:1 rgba(228, 233, 236, 255));
 }

 /* QComboBox gets the "on" state when the popup is open */
 QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.459864, y2:1, stop:0 rgba(185, 196, 200, 255), stop:1 rgba(228, 233, 236, 255));
 }

 QComboBox:on { /* shift the text when the popup opens */
     padding-top: 3px;
     padding-left: 4px;

 }

 QComboBox::drop-down {
     subcontrol-origin: padding;
     subcontrol-position: top right;
     width: 15px;

     border-left-width: 1px;
     border-left-color: darkgray;
     border-left-style: solid; /* just a single line */
     border-top-right-radius: 3px; /* same radius as the QComboBox */
     border-bottom-right-radius: 3px;
 }

 QComboBox::down-arrow {

 }

 QComboBox::down-arrow:on { /* shift the arrow when popup is open */

 }

/*  Group Box Style  */


 QGroupBox {

	padding-top:10px;
	border:1px solid #9eaeb6;
	border-radius:5px;
	border-bottom: 2px solid #797e80;

     margin-top: 1ex; /* leave space at the top for the title */
 }


 QGroupBox::title {
     subcontrol-origin: margin;
     subcontrol-position: top center; /* position at the top center */
     padding: 0 3px;
 }


